  "dependencies": {
    "css": "^2.2.1",
    "postcss": "^5.2.14",
    "postcss-nested": "^1.0.0",
    "postcss-simple-vars": "^3.0.0",
    "stylis": "^1.1.12"
  },
  "devDependencies": {
    "babel": "^6.23.0",
    "babel-cli": "^6.23.0",
    "babel-plugin-transform-async-to-generator": "^6.22.0",
